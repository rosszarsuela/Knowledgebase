    <!-- SMPT -->
	<bean id="mailMessageService" class="com.oris.util.email.SimpleSmtpMessageService"> 
		<property name="config">
			<bean class="com.oris.util.email.SimpleSmtpServiceConfig" >
				<property name="protocol" value="${mail.protocol}"/>
				<property name="host" value="${mail.host}"/>
				<property name="port" value="${mail.port}"/>
				<property name="username" value="${mail.username}"/>
				<property name="password" value="${mail.password}"/>
				<property name="sslEnable" value="${mail.ssl}"/>
				<property name="reqsAuth" value="${mail.auth}"/>
				<property name="startTls" value="${mail.starttls}"/>
			</bean>
		</property>
	</bean>
	
